<menuitem name="Select _All" key="Cmd+A" command="dw.selectAll()" enabled="( dw.getFocus() == 'browser' || dw.getFocus() == 'site' || dw.getFocus() == 'textView' || dw.getFocus(true) == 'html' ||  (dw.getFocus() == 'document' && dw.getDocumentDOM().getFocus() != 'frameset') || dw.getFocus(true) == 'html') && dw.canSelectAll()" domRequired="false" id="DWMenu_Edit_SelectAll" />
<menuitem name="Select Parent Ta_g" key="Cmd+[" command="if (dw.getFocus(true) == 'textView' || dw.getFocus(true) == 'html') { dw.getDocumentDOM().source.selectParentTag(); } else if(dw.getFocus(true) == 'browser'){dw.handleLiveViewSelectParent()} else{ dw.getDocumentDOM().selectParent(); }" enabled="(dw.getFocus() == 'document' && dw.getDocumentDOM().getFocus() != 'head') || (dw.getFocus(true) == 'textView' || dw.getFocus(true) == 'html' || (dw.getFocus(true) == 'browser' && dw.canSelectParent()) )" domRequired="false" id="DWMenu_Edit_SelectParent" />
