<menuitem name="Delete" command="if (dw.getDocumentDOM() && dw.getDocumentDOM().canHandleFGOperation()) { dw.getDocumentDOM().callFGFunction('Delete') } else { dw.handleLiveViewdelete() }" enabled="dw.canHandleLiveViewDelete()" showif="dw.canShowLiveViewDelete()" domRequired="FALSE" id="DWMenu_Edit_Delete" />
