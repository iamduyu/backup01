<menuitem name="Cu_t" key="Cmd+X" command="if (dw.getFocus(true) == 'html' || dw.getFocus() == 'textView') { dw.clipCut(); } else { MM.event.notify('','dw.clipCut()') }" enabled="!(dw.getDocumentDOM() && dw.getDocumentDOM().canHandleFGOperation()) && dw.canClipCut()" domRequired="false" id="DWMenu_Edit_Cut" />
