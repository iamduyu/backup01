<menus xmlns:MMString="http://www.macromedia.com/schemes/data/string/">
<shortcut name="Close Window" key="Cmd+F4" command="if (dw.getFocus() == 'document') { dw.closeDocument(dw.getDocumentDOM()) }" platform="win" id="DWShortcuts_Site_CloseWindow" />
<shortcut name="Quit Application" key="Opt+F4" command="dw.quitApplication()" id="DWShortcuts_Site_Quit" />
<shortcut name="Open" key="Return" arguments="'open','shortcutkey'" file="Menus/MM/Site_File.htm" id="DWShortcuts_Site_SiteOpen" />
<shortcut name="Preview in Primary Browser" key="F12" arguments="'primary'" file="Menus/MM/PIB_Dynamic.htm" platform="win" id="DWShortcuts_Site_PIBPrimary" />
<shortcut name="Preview in Secondary Browser" key="Shift+F12" arguments="'secondary'" file="Menus/MM/PIB_Dynamic.htm" id="DWShortcuts_Site_PIBSecondary" />
<shortcut name="Preview in Device Central" key="Ctrl+Alt+F12" arguments="'devicecentral'" file="Menus/MM/PIB_Dynamic.htm" platform="win" id="DWShortcuts_Site_PIBDevice" />
<shortcut name="Cancel FTP" key="Esc" command="if (dw.getFocus() == 'site') { site.cancelFTPCommand() }" platform="win" id="DWShortcuts_Site_CancelFTP" />
